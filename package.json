{
  "name": "@stackmate/engine",
  "version": "0.1.4",
  "description": "Engine library for Stackmate",
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment",
    "infrastructure-as-code"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/engine"
  },
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/engine/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@bitty/pipe": "^0.3.0",
    "@cdktf/provider-aws": "11.0.10",
    "@cdktf/provider-local": "^4.0.1",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "cdktf": "0.15.2",
    "constructs": "^10.1.241",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.19",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "ts-transform-paths": "^3.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "artifacts/",
    "dist/"
  ],
  "scripts": {
    "bin:schema": "ts-node -r tsconfig-paths/register src/bin/json-schema.ts",
    "build": "yarn clean && yarn bin:schema && yarn build:project && yarn build:aliases",
    "build:aliases": "tsc-alias -p tsconfig.build.json",
    "build:project": "tsc -p ./tsconfig.build.json",
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "test": "jest --verbose --useStderr --detectOpenHandles --silent=false",
    "test:full": "yarn bin:schema && yarn test",
    "test:watch": "yarn test --watchAll"
  }
}
