{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "inputs": ["src/**/*.ts", "tests/**/*.test.ts"],
      "outputs": []
    },
    "test:watch": {
      "inputs": ["src/**/*.ts", "tests/**/*.test.ts"],
      "outputs": []
    }
  }
}
