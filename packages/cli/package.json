{
  "name": "@stackmate/cli",
  "version": "0.0.0",
  "description": "Stackmate - launch and deploy cloud infrastructure using 1-line commands",
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bin": {
    "stackmate": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/stackmate/issues"
  },
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/stackmate",
    "directory": "packages/cli"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.6.1",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@stackmate/engine": "^0.1.0",
    "chalk": "^5.0.1",
    "ffi-napi": "^4.0.3",
    "lodash": "4.17.21",
    "ts-node": "^10.7.0",
    "typescript-memoize": "^1.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/ffi-napi": "^4.0.5",
    "@types/mocha": "^9.0.0",
    "mocha": "^9.2.2",
    "oclif": "^2.6.0"
  },
  "oclif": {
    "bin": "stackmate",
    "dirname": "stackmate",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "deploy": {
        "description": "Deploy your infrastructure to the cloud"
      }
    }
  },
  "scripts": {
    "go:bindings": "go build -o bin/terraform.so -buildmode=c-shared src/lib/terraform/bindings.go",
    "build": "rimraf dist/* && rollup -c rollup.config.js && oclif manifest",
    "lint": "eslint . --ext .ts",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif readme",
    "test": "mocha --forbid-only \"tests/**/*.test.ts\"",
    "manifest": "oclif manifest",
    "command:dev": "./bin/dev",
    "command:run": "./bin/run",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
