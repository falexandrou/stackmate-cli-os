{
  "name": "@stackmate/cli",
  "version": "0.1.4",
  "description": "Stackmate - launch and deploy cloud infrastructure using 1-line commands",
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bin": {
    "stackmate": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/stackmate/issues"
  },
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/stackmate",
    "directory": "packages/cli"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/color": "^1.0.4",
    "@oclif/core": "^1.26.1",
    "@oclif/plugin-help": "^5.2.4",
    "@oclif/plugin-plugins": "^2.4.7",
    "@stackmate/engine": "^0.1.4",
    "chalk": "^5.2.0",
    "ini": "^3.0.1",
    "inquirer": "^8.2.5",
    "lodash": "4.17.21",
    "oclif": "3.6.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.19",
    "jest": "^29.4.1",
    "oclif": "^3.6.2",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "turbo": "^1.7.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "stackmate",
    "dirname": "stackmate",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "deploy": {
        "description": "Deploy your infrastructure to the cloud"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "build": "yarn clean && yarn build:project && yarn build:aliases",
    "build:project": "tsc -p tsconfig.build.json",
    "build:aliases": "tsc-alias -p tsconfig.build.json",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif manifest && oclif readme",
    "manifest": "oclif manifest",
    "test": "jest --verbose --useStderr --detectOpenHandles --silent=false",
    "test:watch": "yarn test --watchAll"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
