{
  "name": "@stackmate/engine",
  "version": "0.1.4",
  "description": "Engine library for Stackmate",
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/stackmate",
    "directory": "packages/engine"
  },
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/stackmate/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@bitty/pipe": "^0.3.0",
    "@cdktf/provider-aws": "11.0.0",
    "@cdktf/provider-local": "^4.0.0",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "cdktf": "0.14.1",
    "constructs": "^10.1.167",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.13",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "artifacts/",
    "dist/"
  ],
  "scripts": {
    "bin:schema": "ts-node -r tsconfig-paths/register src/bin/json-schema.ts",
    "build": "yarn clean && yarn bin:schema && yarn build:project && yarn build:aliases",
    "build:aliases": "tsc-alias -p tsconfig.build.json",
    "build:project": "tsc -p ./tsconfig.build.json",
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "test": "jest --verbose --useStderr --detectOpenHandles --silent=false",
    "test:full": "yarn bin:schema && yarn test",
    "test:watch": "yarn test --watchAll"
  }
}
