{
  "name": "@stackmate/engine",
  "version": "0.0.0",
  "description": "Engine library for Stackmate",
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/stackmate",
    "directory": "packages/engine"
  },
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/stackmate/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@bitty/pipe": "^0.3.0",
    "@cdktf/provider-aws": "10.0.8",
    "@cdktf/provider-local": "^3.0.8",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "cdktf": "0.13.0",
    "constructs": "^10.1.127",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.3",
    "jest": "^29.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "build": "yarn clean && yarn schema:generate && yarn build:project && yarn build:aliases",
    "build:aliases": "tsc-alias -p tsconfig.build.json",
    "build:project": "tsc -p ./tsconfig.build.json",
    "schema:generate": "ts-node -r tsconfig-paths/register src/bin/json-schema.ts",
    "schema:generate:dist": "yarn schema:generate ./dist/stackmate.schema.json",
    "test": "jest --verbose --useStderr --detectOpenHandles --silent=false",
    "test:full": "yarn schema:generate && yarn test",
    "test:watch": "yarn test --watchAll"
  }
}
