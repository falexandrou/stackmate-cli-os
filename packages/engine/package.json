{
  "name": "@stackmate/engine",
  "version": "0.0.0",
  "description": "Engine library for Stackmate",
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning",
    "deployment"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackmate-io/stackmate",
    "directory": "packages/engine"
  },
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/stackmate/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cdktf/provider-aws": "5.0.52",
    "@cdktf/provider-local": "^0.5.15",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "cdktf": "0.9.4",
    "constructs": "^10.1.32",
    "ip-address": "^8.1.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "typescript-memoize": "^1.1.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@types/jest": "27.5.1",
    "jest": "27.5.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "rimraf dist/* && yarn schema:generate && rollup -c rollup.config.js",
    "schema:generate": "ts-node -P tsconfig.tsnode.json -r tsconfig-paths/register src/schema.ts",
    "test": "rimraf tests/output/* && yarn schema:generate && jest --bail --verbose --detectOpenHandles --watchAll",
    "test:ci": "rimraf tests/output/* && yarn schema:generate && jest --bail --verbose --detectOpenHandles"
  }
}
