{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "yaml.schemas": {
    "dist/schema.json": "examples/*.yml",
  },
  "files.restoreUndoStack": true,
  "editor.rulers": [100],
  "eslint.validate": [
    "typescript",
    "javascript"
  ]
}
